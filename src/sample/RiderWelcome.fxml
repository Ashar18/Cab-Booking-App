<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="477.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.RiderWelcome">
   <children>
      <ImageView fitHeight="35.0" fitWidth="43.0" layoutX="452.0" layoutY="489.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/icons8-refresh-90.png" />
         </image>
      </ImageView>
      <Button fx:id="button_refresh" alignment="BASELINE_LEFT" graphicTextGap="30.0" layoutX="441.0" layoutY="490.0" mnemonicParsing="false" onAction="#refreshOnAction" opacity="0.0" prefHeight="31.0" prefWidth="55.0" style="-fx-background-color: #e9e9e9;" underline="true">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <padding>
            <Insets left="10.0" />
         </padding>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <ImageView fitHeight="386.0" fitWidth="464.0" layoutX="241.0" layoutY="-14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/rider.jpg" />
         </image>
      </ImageView>
      <Button fx:id="button_ok" layoutX="424.0" layoutY="446.0" mnemonicParsing="false" onAction="#okOnAction" prefHeight="31.0" prefWidth="82.0" style="-fx-background-color: #000000; -fx-border-radius: 6;" text="OK" textFill="#fabd02">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <MenuButton fx:id="loc" layoutX="357.0" layoutY="391.0" mnemonicParsing="false" onAction="#nkOnAction" prefHeight="37.0" prefWidth="220.0" text="Current Location...">
         <items>
            <MenuItem fx:id="nk" mnemonicParsing="false" onAction="#nkOnAction" text="North Karachi" />
            <MenuItem fx:id="nn" mnemonicParsing="false" onAction="#nnOnAction" text="North Nazimabad" />
            <MenuItem fx:id="gi" mnemonicParsing="false" onAction="#giOnAction" text="Gulshan Iqbal" />
            <MenuItem fx:id="bf" mnemonicParsing="false" onAction="#bfOnAction" text="Bufferzone" />
         </items>
         <font>
            <Font size="15.0" />
         </font>
      </MenuButton>
      <VBox prefHeight="532.0" prefWidth="180.0" style="-fx-background-color: ffc000;">
         <children>
            <ImageView fitHeight="101.0" fitWidth="174.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/logo.jpeg" />
               </image>
               <VBox.margin>
                  <Insets left="10.0" top="10.0" />
               </VBox.margin>
            </ImageView>
            <VBox prefHeight="46.0" prefWidth="180.0" />
            <Button fx:id="button_profile" alignment="BASELINE_LEFT" graphicTextGap="30.0" mnemonicParsing="false" onAction="#profileOnAction" prefHeight="44.0" prefWidth="188.0" style="-fx-background-color: FABD02;" text="Profile" underline="true">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <padding>
                  <Insets left="60.0" />
               </padding>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button fx:id="button_history" alignment="BASELINE_LEFT" graphicTextGap="30.0" mnemonicParsing="false" onAction="#historyOnAction" prefHeight="41.0" prefWidth="258.0" style="-fx-background-color: FABD02;" text="My Trips" underline="true">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <padding>
                  <Insets left="60.0" />
               </padding>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button fx:id="button_logout" alignment="BASELINE_LEFT" graphicTextGap="30.0" layoutX="10.0" layoutY="194.0" mnemonicParsing="false" onAction="#logoutOnAction" prefHeight="41.0" prefWidth="258.0" style="-fx-background-color: FABD02;" text="Logout" underline="true">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <padding>
                  <Insets left="60.0" />
               </padding>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
